#include <bits/stdc++.h>
#define MAX 10000

using namespace std;

int v[MAX];
int w[MAX];

int k[MAX][MAX];
int knap(int n, int weight){

	for(int i = 0 ; i <= n ; i++)
		for(int j = 0 ; j <= weight; j++){
			if(i == 0 || j == 0) k[i][j] = 0;
			else if (w[i-1] <= j)
			k[i][j] = max(v[i-1] + k[i-1][j-w[i-1]],k[i-1][j]);
			else k[i][j] = k[i-1][j];
		}

	return k[n][weight];
}

int knap2(int n, int weight){ 

	int K[MAX];
	K[0] = 0;
	int best = 0;
	for (int k = 1; k <= weight; k++) {
		for (int i = 1; i <= n; i++) {
			if (k >= w[i]) {
				best = max(K[k-w[i]] + v[i], best);
			 }
		}
		K[k] = best;
	}
   
  return K[weight];
}

int main() {

	int n,m;
	scanf("%d %d",&n,&m);
	for(int i = 1; i <= n; i++){
		scanf("%d %d",&w[i],&v[i]);
	}

	printf("%d\n",knap2(n,m));
	return 0;
}
