#include <bits/stdc++.h>
#define MAX 3000
using namespace std;

double dist, inicio, fim;
int d[MAX], a[MAX], b[MAX];
int n;
int main() {

	
	scanf("%d", &n);
	for(int i = 0; i < n; i++){
		scanf("%d %d %d",&d[i], &a[i], &b[i]);
        }

	bool can = true;
	cin >> dist >> inicio >> fim;
	for(int i = 0; i < n; i++) {
		double maxH = (d[i]*b[i])/(1.0*dist);
		double minH = (d[i]*a[i])/(1.0*dist);
		if(maxH < inicio || minH > fim) can = false;
		if(maxH < fim) fim = maxH;
		if(minH > inicio) inicio = minH;
		
	}

	printf("%c\n",can ? 'Y' : 'N');
	return 0;
}
