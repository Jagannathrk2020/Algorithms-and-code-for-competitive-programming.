#include <bits/stdc++.h>

using namespace std;

int read(int idx){
    int sum = 0;
    while (idx > 0){
        sum += tree[idx];
        idx -= (idx & -idx);
    }
    return sum;
}

void update(int idx ,int val){
    while (idx <= MaxVal){
        tree[idx] += val;
        idx += (idx & -idx);
    }
}

void updatex(int x , int y , int val){
    while (x <= max_x){
        updatey(x , y , val); 
        // this function should update array tree[x] 
        x += (x & -x); 
    }
}

void updatey(int x , int y , int val){
    while (y <= max_y){
        tree[x][y] += val;
        y += (y & -y); 
    }
}

int main() {

	scanf("%d",&n);
	int maxVal = 0;
	for(int i = 0; i < n; i++) {scanf("%d",&a[i]); maxVal = max(a[i],maxVal);}
	


}
